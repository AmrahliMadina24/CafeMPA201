@model List<ChefGetVM>
<div class="cotainer">
    <div class="row">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">FullName</th>
                    <th scope="col">Image</th>
                    <th scope="col">Position Name</th>
                    <th scope="col">Actions</th>
                </tr>
            </thead>
            <tbody>
              
                    @foreach(var chef in Model){
                        <tr>
                    <th scope="row">@chef.Id</th>
                    <td>@chef.FullName</td>
                    <td><img src="~/imges/@chef.ImagePath" /></td>
                    <td>@chef.PositionName</td>
                    <td>
                        <a asp-action="Update" asp-route-id="@chef.Id" class="btn btn-warning">Update</a>
                        <a asp-action="Delete" asp-route-id="@chef.Id" class="btn btn-danger">Delete</a>
                    </td>
                   </tr>
                    }
                   
               
               
            </tbody>
        </table>
    </div>
</div>